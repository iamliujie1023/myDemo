<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundAngleFrameLayout">
        <attr name="radius" format="dimension" />
        <attr name="topLeftRadius" format="dimension" />
        <attr name="topRightRadius" format="dimension" />
        <attr name="bottomLeftRadius" format="dimension" />
        <attr name="bottomRightRadius" format="dimension" />
    </declare-styleable>


    <declare-styleable name="PTextView">
        <attr name="tag_color" format="color" />
        <attr name="price_color" format="color" />
        <attr name="price_size" format="dimension" />
        <attr name="tag_size" format="dimension" />
        <attr name="is_origin" format="boolean" />
        <attr name="is_bold" format="boolean" />
        <attr name="is_decimal" format="boolean" />
    </declare-styleable>
</resources>